{
  "$schema": "../schema/ItemSchema.json",
  "Beleuchtung": {
    "type": "str",
    "visu_acl": "ro",
    "eval": "str(~.AutoLight.Rules)",
    "eval_trigger": "~",
    "Parameter": {
      "Nachlauf": {
        "$order": 1,
        "type": "num",
        "visu_acl": "rw",
        "cache": true,
        "value": "10"
      }
    },
    "AutoLight": {
        "$order": 10,
        "$template": {
            "source": "autolight"
        }
    },
     "Taster_Nacht": {
        "$order": 10,
        "type": "bool",
        "visu": "yes",
        "visu_acl": "rw",
        "enforce_updates": true,
        "knx_dpt": "1",
        "knx_send": "7/-/+5",
        "knx_listen": "7/-/+5",
        "cache": true
    },
  }
}

