                "cc_plugin": "active",
                "Zaehler_jalousie": {
                    "cc_count_counter_Jalousie": 4
                }

            },
            "State": {
                "Auf_ab": {
                    "Links": {
                        "eval": "~~() if (~~~.Zaehler_jalousie() == 0 or ~~~.Zaehler_jalousie() == 1) else None",
                        "eval_trigger": "~~",
                        "set_item": {
                            "type": "bool",
                            "eval": "~~()",
                            "eval_trigger": "~~",
                            "item": "sh.Eg.Buero.Jalousie.Links.Auf_ab"
                        }
                    },
                    "Mitte": {
                        "eval": "~~() if (~~~.Zaehler_jalousie() == 0 or ~~~.Zaehler_jalousie() == 2) else None",
                        "eval_trigger": "~~",
