                        "set_item": {
                            "type": "bool",
                            "eval": "~~()",
                            "eval_trigger": "~~",
                                "item":"sh.Dg.Flur.Dachfenster.Lamellenverstellung_stop" 
                        }
                    }
                }
            },

            "Leds": {
                "$templated": true,
                "Led1Prio": {
                    "eval": "True if (~~~.State.Zaehler_jalousie()==1 or ~~~.State.Zaehler_jalousie()==2)  else False",
                    "eval_trigger": "~~~.State.Zaehler_jalousie"
                },
                "Led2Prio": {
                    "eval": "True if (~~~.State.Zaehler_jalousie()==1 or ~~~.State.Zaehler_jalousie()==2)  else False",
