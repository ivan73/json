                "type": "num",
                "visu_acl": "ro",
                "cache": true,
                "eval": "~~.Strom()* 230 / 1000 / 24",
                "eval_trigger": "~~.Strom",
                "sqlite": "yes"
            },
            "Leistung": {
                "type": "num",
                "visu_acl": "ro",
                "cache": true,
                "eval": "~~.Strom()* 230 / 1000",
                "eval_trigger": "~~.Strom",
