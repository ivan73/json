                "source": "clickcontrol",
                "knx": "6/2/111"
            },
            "Rules": {
                "Zaehler_jalousie": {
                    "cc_count_counter_Jalousie": 2
                }

            },
            "State": {
                "Auf_ab": {
                    "Fenster": {
                        "eval": "~~() if ( ~~~.Zaehler_jalousie() == 0)",
                        "eval_trigger": "~~",
                        "set_item": {
                            "type": "bool",
                            "eval": "~~()",
                            "eval_trigger": "~~",
                             "item":"sh.Dg.Flur.Jalousie.Fenster.Auf_ab" 
                        }
                    },
                    "Dachfenster": {
                        "eval": "~~() if ( ~~~.Zaehler_jalousie() == 1)",
                        "eval_trigger": "~~",
                        "set_item": {
                            "type": "bool",
                            "eval": "~~()",
                            "eval_trigger": "~~",
                                "item":"sh.Dg.Flur.Dachfenster.Auf_ab" 
