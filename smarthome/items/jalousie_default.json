      "enter_manuell": {
        "as_value_PositionGueltig": "False",
        "as_value_trigger_source": "eval: autoblind_eval.get_relative_itemid('..manuell_positionen')"
      },
      "enter_stay": {
        "as_value_laststate": "var:current.state_id",
        "as_value_PositionGueltig": "False"
      }
    },
    "PositionUngueltig": {
      "$order": 12,
