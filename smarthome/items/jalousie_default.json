      "type": "foo",
      "name": "Automatik AUS",
      "as_set_suspend": "False",
      "enter": {
        "as_value_automatik": "False"
      }
    },

    "Referenzfahrt_ende": {
      "$order": 10,
      "type": "foo",
      "name": "Ende Referenzfahrt",
      "as_set_ReferenzFahren": "False",
      "as_value_ReferenzPruefen": "0",
